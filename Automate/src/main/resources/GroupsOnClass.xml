<?xml version="1.0" encoding="UTF-8" ?>
<suite name="suitename">
    <!-- 组测试中的类分组测试 -->
    <test name="runAll">
        <classes>
            <class name="com.interfacetesting.Testng.groups.GroupsOnClass1" />
            <class name="com.interfacetesting.Testng.groups.GroupsOnClass2"/>
            <class name="com.interfacetesting.Testng.groups.GroupsOnClass3"/>
        </classes>
    </test>

    <test name="onlyRunStu">
        <groups>
            <run>
                <include name="stu" />
            </run>
        </groups>
        <classes>
            <class name="com.interfacetesting.Testng.groups.GroupsOnClass1" />
            <class name="com.interfacetesting.Testng.groups.GroupsOnClass2"/>
            <class name="com.interfacetesting.Testng.groups.GroupsOnClass3"/>
        </classes>
    </test>

</suite>